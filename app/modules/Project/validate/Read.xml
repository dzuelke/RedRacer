<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
	xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
	xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	parent="%core.module_dir%/Project/config/validators.xml"
>
	<ae:configuration>
		
		<validators method="read">

			<validator class="AgaviNotoperatorValidator">
				<validator class="RedracerUniqueFieldValueValidator">
					<argument>project</argument>
					<ae:parameters>
						<ae:parameter name="table_manager">Project.Manager</ae:parameter>
						<ae:parameter name="field_name">name</ae:parameter>
						<ae:parameter name="export_results">true</ae:parameter>
					</ae:parameters>
				</validator>
			</validator>

		</validators>
		
	</ae:configuration>
</ae:configurations>
