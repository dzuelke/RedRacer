<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
	xmlns="http://agavi.org/agavi/config/parts/rbac_definitions/1.0">
	<ae:configuration>

		<roles>
			<role name="Member">
				<permissions>
					<permission>changepassword</permission>
				</permissions>

				<roles>
					<role name="Developer">
						<permissions>
							<permission>approve</permission>
						</permissions>

						<roles>
							<role name="Administrator">
								<permissions>
									<permission>all</permission>
								</permissions>
							</role>
						</roles>
					</role>
				</roles>
			</role>
		</roles>
	</ae:configuration>
</ae:configurations>